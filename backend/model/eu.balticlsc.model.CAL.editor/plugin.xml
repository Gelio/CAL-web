   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated CAL -->
      <editor
            id="eu.balticlsc.model.CAL.presentation.CALEditorID"
            name="%_UI_CALEditor_label"
            icon="icons/full/obj16/CALModelFile.gif"
            extensions="cal"
            class="eu.balticlsc.model.CAL.presentation.CALEditor"
            contributorClass="eu.balticlsc.model.CAL.presentation.CALActionBarContributor">
