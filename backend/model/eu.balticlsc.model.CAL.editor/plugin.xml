<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated CAL -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="eu.balticlsc.model.CAL.presentation.CALModelWizardID"
            name="%_UI_CALModelWizard_label"
            class="eu.balticlsc.model.CAL.presentation.CALModelWizard"
